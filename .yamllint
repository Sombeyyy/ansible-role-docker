---
extends: default

rules:
  line-length:
    max: 150
    level: warning
  brackets:
    max-spaces-inside: 1
    level: warning
  braces:
    max-spaces-inside: 1
    level: warning
  comments:
    min-spaces-from-content: 1
  comments-indentation: false
  octal-values:
    forbid-implicit-octal: true
    forbid-explicit-octal: true
